    "lint:fix": "eslint \"./src/**/*.{js,jsx,ts,tsx}\" --fix",
    "preview": "vite preview",
    "format": "prettier --write 'src/**/*.{js,ts,jsx,tsx,scss,css,html,json,md}'",
